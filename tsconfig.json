{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "strict": true,
    "verbatimModuleSyntax": true,
    "jsxImportSource": "solid-js",
    "jsx": "preserve",
    "strictNullChecks": true
  }
}